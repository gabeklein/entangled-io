{
  "name": "entangled-io",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm:start:*\"",
    "prepare": "lerna bootstrap",
    "build": "lerna run build",
    "start:api": "npm start --prefix example/api",
    "start:client": "npm start --prefix example/client",
    "watch:client": "npm run watch --prefix example/client",
    "build:client": "npm run build --prefix example/client"
  },
  "workspaces": [
    "packages/*",
    "example/*"
  ],
  "devDependencies": {
    "@types/node": "^14.6.0",
    "concurrently": "^5.1.0",
    "lerna": "^4.0.0",
    "tsup": "^8.0.1",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "source-map-support": "^0.5.16",
    "ts-node": "^10.4.0"
  }
}
