{
  "compilerOptions": {
    "outDir": "./lib",
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": ["DOM", "ESNext"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true
  }
}